{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "papyrus",
      "camelcase": "Papyrus",
      "title": "Papyrus",
      "scope": "source.papyrus",
      "file-types": [
        "psc"
      ],
      "injection-regex": "^papyrus$",
      "class-name": "TreeSitterPapyrus"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Papyrus grammar for tree-sitter",
    "authors": [
      {
        "name": "ayooh",
        "url": "https://github.com/ayooh"
      }
    ],
    "links": {
      "repository": "https://github.com/ayooh/tree-sitter-papyrus",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
